{
  "features": [
    {
      "name": "smart-booking-system",
      "title": "Smart Booking System",
      "description": "Intuitive calendar interface with real-time availability and conflict detection",
      "priority": 1,
      "actions": [
        {
          "type": "navigate",
          "url": "/dashboard"
        },
        {
          "type": "wait",
          "duration": 2000
        },
        {
          "type": "click",
          "selector": "[data-testid=\"calendar-view\"]"
        },
        {
          "type": "wait",
          "duration": 1000
        },
        {
          "type": "click",
          "selector": "[data-testid=\"new-booking\"]"
        },
        {
          "type": "wait",
          "duration": 2000
        },
        {
          "type": "fill",
          "selector": "[data-testid=\"customer-name\"]",
          "value": "John Smith"
        },
        {
          "type": "select",
          "selector": "[data-testid=\"room-select\"]",
          "value": "Room A"
        },
        {
          "type": "click",
          "selector": "[data-testid=\"save-booking\"]"
        },
        {
          "type": "wait",
          "duration": 3000
        }
      ]
    },
